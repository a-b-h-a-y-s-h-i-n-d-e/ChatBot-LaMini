{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"  
        }
      },
      {
        "src": "main.py",  
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "main.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "frontend/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"  
      }
    ]
  }
  